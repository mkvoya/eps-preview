{
	"name": "eps-preview",
	"displayName": "eps-preview",
	"description": "Preview EPS figure files",
	"version": "0.5.0",
	"publisher": "mkvoya",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:eps-preview.sidePreview",
		"onLanguage:postscript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "eps-preview.sidePreview",
				"title": "Preview EPS",
				"icon": "$(open-preview)"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == postscript",
					"command": "eps-preview.sidePreview",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "EPS-Preview",
			"properties": {
				"eps-preview.path.ps2pdf": {
					"type": [
						"string",
						"null"
					],
					"default": "ps2pdf",
					"description": "The path to the ps2pdf binary."
				},
				"eps-preview.path.pdf2svg": {
					"type": [
						"string",
						"null"
					],
					"default": "pdf2svg",
					"description": "The path to the pdf2svg binary."
				},
				"eps-preview.backgroundColor": {
					"type": "string",
					"default": "white",
					"description": "The background color of the preview."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"lodash": "^4.17.21",
		"mocha": "^10.2.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/temp": "^0.8.34",
		"temp": "^0.9.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mkvoya/eps-preview.git"
	}
}
