{
	"name": "eps-preview",
	"displayName": "eps-preview",
	"description": "Preview EPS figure files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:eps-preview.sidePreview",
		"onCustomEditor:Preview.EPS"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "eps-preview.sidePreview",
				"title": "Preview EPS"
			}
		],
		"languages": [
			{
				"id": "eps",
				"extensions": [
					".eps"
				],
				"aliases": [
					"eps",
					"EPS"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"customEditors": [
			{
				"viewType": "Preview.EPS",
				"displayName": "EPS",
				"selector": [
					{
						"filenamePattern": "*.eps"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/temp": "^0.8.34",
		"temp": "^0.9.1"
	}
}
